# bootstrap form
  'Bootstrap form':
    'prefix': 'bform'
    'body': """
      <form class="form-horizontal">
        $1
      </form>
    """
  'Bootstrap form input':
    'prefix': 'bform-input'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4$3">$1</label>
        <div class="col-md-8$3">
          <div class="input-group">
            <input type="text" class="form-control" $2>
          </div>
        </div>
      </div>
    """
  'Bootstrap form input with btn':
    'prefix': 'bform-input-btn'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4$3">$1</label>
        <div class="col-md-8$3">
          <div class="input-group">
            <input type="text" class="form-control" $2>
            <span class="input-group-btn">
              <button type="button" class="btn btn-primary btn-flat">选择</button>
            </span>
          </div>
        </div>
      </div>
      """
  'Bootstrap form radio':
    'prefix': 'bform-radio'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4$3">$1</label>
        <div class="col-md-8$3">
          <div class="controls">
            <div class="radio inline">
              <label><input type="radio" value="1">$1</label>
            </div>
            <div class="radio inline">
              <label><input type="radio" value="2">$2</label>
            </div>
          </div>
        </div>
      </div>
    """
  'Bootstrap form checkbox':
    'prefix': 'bform-checkbox'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4$5">$1</label>
        <div class="col-md-8$4">
          <div class="controls">
            <div class="checkbox inline">
              <label>
                <input type="checkbox">
                $2
              </label>
            </div>
            <div class="checkbox inline">
              <label>
                <input type="checkbox">
                $3
              </label>
            </div>
            <div class="checkbox inline">
              <label>
                <input type="checkbox">
                $4
              </label>
            </div>
          </div>
        </div>
      </div>
    """
  'Bootstrap form text':
    'prefix': 'bform-text'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4$3">$1</label>
        <div class="col-md-8$3">
          <p class="form-control-static">$2</p>
        </div>
      </div>
    """
  'Bootstrap form select':
    'prefix': 'bform-select'
    'body': """
      <div class="form-group">
        <label class="control-label col-md-4">$1</label>
        <div class="col-md-8">
          <select class="form-control">
            <option>$2</option>
            <option>$3</option>
          </select>
        </div>
      </div>
    """
  'Bootstrap form textarea':
    'prefix': 'bform-textarea'
    'body': """
      <div class="form-group">
