    """

# bootstrap row
  'Bootstrap row':
    'prefix': 'brow'
    'body': """
        <div class="row">
          <div class="col-md-12$2">
            $1
          </div>
        </div>
    """

# bootstrap col
  'Bootstrap col':
    'prefix': 'bcol'
