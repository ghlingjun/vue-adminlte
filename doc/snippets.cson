  'Notify success':
    'prefix': 'notify-success'
    'body': """$notify.success('$1')"""
  'Notify info':
    'prefix': 'notify-info'
    'body': """$notify.info('$1')"""
  'Notify warn':
    'prefix': 'notify-warn'
    'body': """$notify.warn('$1')"""
  'Notify danger':
    'prefix': 'notify-danger'
    'body': """$notify.danger('$1')"""
  'confirm':
    'prefix': 'confirm'
    'body': """$confirm('$1', function () {
        $2
      })
    """
  'Dict Select':
    'prefix': 'dict-select'
    'body': """
    <DictSelect v-model="$2" type="$1"></DictSelect>
    """

