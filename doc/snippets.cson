####### 容器类
######################################################
# bootstrap section
  'Bootstrap section':
    'prefix': 'bsection'
    'body': """
      <section class="content">
        <div class="row">
          <div class="col-md-12$2">
            $1
          </div>
        </div>
      </section>
    """

# bootstrap box
  'Bootstrap box':
    'prefix': 'bbox'
    'body': """
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">$1</h3>
        </div>
        <div class="box-body">
          $2
        </div>
        <div class="box-footer">
          $3
        </div>
      </div>
    """

# bootstrap box
